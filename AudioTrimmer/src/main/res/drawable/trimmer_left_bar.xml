<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#fff" />
            <corners
                    android:bottomLeftRadius="2dp"
                    android:topLeftRadius="2dp" />
            <size
                    android:width="10dp"
                    android:height="39dp" />
        </shape>
    </item>
    <item android:left="1dp" android:gravity="center_vertical|left">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/black" />
            <size
                android:width="2dp"
                android:height="9dp" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/black" />
            <size
                android:width="2dp"
                android:height="9dp" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item android:right="1dp" android:gravity="center_vertical|right">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/black" />
            <size
                android:width="2dp"
                android:height="9dp" />
            <corners android:radius="2dp" />
        </shape>
    </item>


</layer-list>